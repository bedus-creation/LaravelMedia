{
    "name": "aammui/laravel-media",
    "description": "File and Image uploader for laravel.",
    "keywords": [
        "aammui",
        "media",
        "media-uploader",
        "file-uploader"
    ],
    "authors": [
        {
            "name": "Bedram Tamang",
            "email": "tmgbedu@gmail.com"
        }
    ],
    "homepage": "https://github.com/bedus-creation/MediaHandle",
    "license": "MIT",
    "type": "library",
    "require": {
        "php":                 "^7.2.5|^8.0",
        "illuminate/http":     "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support":  "^6.0|^7.0|^8.0",
        "intervention/image":  "^2.5",
        "ext-json":              "*",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Aammui\\LaravelMedia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aammui\\LaravelMedia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aammui\\LaravelMedia\\LaravelMediaServiceProvider"
            ]
        }
    }
}
