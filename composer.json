{
    "name": "aammui/laravel-media",
    "description": "File and Image uploader for laravel.",
    "keywords": [
        "aammui",
        "media",
        "media-uploader",
        "file-uploader"
    ],
    "authors": [
        {
            "name": "Bedram Tamang",
            "email": "tmgbedu@gmail.com"
        }
    ],
    "homepage": "https://github.com/bedus-creation/MediaHandle",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-gd": "*",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aammui\\LaravelMedia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aammui\\LaravelMedia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aammui\\LaravelMedia\\LaravelMediaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
